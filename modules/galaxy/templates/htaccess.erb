RewriteEngine on
RewriteRule ^/static/style/(.*) <%= @project_dir %>/static/june_2007_style/blue/$1 [L]
RewriteRule ^/static/scripts/(.*) <%= @project_dir %>/static/scripts/packed/$1 [L]
RewriteRule ^/static/(.*) <%= @project_dir %>/static/$1 [L]
RewriteRule ^/favicon.ico <%= @project_dir %>/static/favicon.ico [L]
RewriteRule ^/robots.txt <%= @project_dir %>/static/robots.txt [L]
RewriteRule ^(.*) balancer://galaxy/$1 [P]
